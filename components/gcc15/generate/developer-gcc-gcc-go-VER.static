    mediator=gcc mediator-version=$(GCC_MAJOR_VERSION)
link path=usr/bin/go target=../lib/go-gcc mediator=go \
    mediator-implementation=gcc
link path=usr/bin/gofmt target=../lib/gofmt-gcc mediator=go \
    mediator-implementation=gcc
