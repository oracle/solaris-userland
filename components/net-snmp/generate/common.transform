# By default everything goes to base package
<transform file link -> default pkg.type base>

# Separate python files
<transform file link path=.*/python3.11/.* -> set pkg.type py311>

# ... demo files
<transform file path=sun/sdk/demo -> set pkg.type demo>

# ... addon files
<transform file path=.*/libseaExtensions.so -> set pkg.type addons>

# These files are handled in *.static
<transform file path=.*/libseaExtensions.so -> drop>
<transform file path=.*/libentity.so -> drop>
