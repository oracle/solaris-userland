user username=_buildbot ftpuser=false gcos-field="Buildbot Reserved UID" \
    group=_buildbot home-dir=/var/share/buildbot-worker uid=74
license COPYING license=GPLv2

# force a dependency on the Python runtime
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# force a dependency on twisted; pkgdepend work is needed to flush this out.
depend type=require fmri=library/python/python-twisted-$(PYV)
