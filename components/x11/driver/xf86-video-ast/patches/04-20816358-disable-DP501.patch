-	    pFWAddr = AST_DP501_firmware;
-            Len     = sizeof(AST_DP501_firmware) / sizeof(AST_DP501_firmware[0]);
-	}
-
-        /* Get BootAddress */
-        MOutdwm(mmiobase, 0x1e6e2000, 0x1688a8a8);				/* open passwd */
-        Data = MIndwm(mmiobase, 0x1e6e0004);
